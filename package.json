{
  "name": "nextjs-template",
  "description": "Next.js template with lints & test",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/stscoundrel/nextjs-template.git",
  "author": "stscoundrel <silvennoinen.sampo@gmail.com>",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint src tests",
    "fix": "eslint src tests --fix",
    "jest": "jest --verbose --runInBand --passWithNoTests",
    "test:unit": "yarn jest",
    "test:integration": "yarn cypress run --env env=test --spec 'tests/integration/tests/**/*'",
    "test:coverage": "jest --coverage tests src/components --coverageThreshold='{\"global\":{\"statements\":\"90\", \"functions\":\"80\", \"branches\":\"80\", \"lines\":\"90\"}}'"
  },
  "dependencies": {
    "abbreviatrix": "^1.0.6",
    "markari": "^1.0.6",
    "next": "^12.2.5",
    "next-pwa": "^5.5.4",
    "old-norse-alphabet": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reset-css": "^5.0.1",
    "sass": "^1.54.8",
    "sitemap": "^7.1.1",
    "slugify": "^1.6.5",
    "teljari": "^1.1.1",
    "viking-language-dictionary": "^0.10.1",
    "viking-language-dictionary-abbreviations": "^0.9.0",
    "younger-futhark": "^1.2.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cypress": "^10.4.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.2.5",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jafningjar": "^1.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.0.1",
    "next-sitemap": "^3.1.16",
    "node-mocks-http": "^1.11.0",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
